alias sapt='sudo apt-get'
alias aptall='sapt update -y && sapt upgrade -y && sapt autoremove -y && sapt autoclean'
alias topten='history | awk '\''{print $2}'\'' | awk '\''BEGIN {FS="|"}{print $1}'\'' | sort | uniq -c | sort -n | tail | sort -nr'
