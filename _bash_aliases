alias dhumnt="fusermount -u /media/dhbw-account/"
alias comp-jpg='find . -iname "*.jp*g" -type f -print0 | xargs -0 jpegoptim -t'
alias comp-png='find . -iname "*.png" -type f -print0 | xargs -0 optipng'
